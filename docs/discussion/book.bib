@article{gloag_bacterial_2015,
	title = {Bacterial {Stigmergy}: {An} {Organising} {Principle} of {Multicellular} {Collective} {Behaviours} of {Bacteria}},
	volume = {2015},
	shorttitle = {Bacterial {Stigmergy}},
	url = {http://www.hindawi.com/journals/scientifica/2015/387342/abs/},
	doi = {10.1155/2015/387342},
	abstract = {The self-organisation of collective behaviours often manifests as dramatic patterns of emergent large-scale order. This is true for relatively \&\#x201c;simple\&\#x201d; entities such as microbial communities and robot \&\#x201c;swarms,\&\#x201d; through to more complex self-organised systems such as those displayed by social insects, migrating herds, and many human activities. The principle of stigmergy describes those self-organised phenomena that emerge as a consequence of indirect communication between individuals of the group through the generation of persistent cues in the environment. Interestingly, despite numerous examples of multicellular behaviours of bacteria, the principle of stigmergy has yet to become an accepted theoretical framework that describes how bacterial collectives self-organise. Here we review some examples of multicellular bacterial behaviours in the context of stigmergy with the aim of bringing this powerful and elegant self-organisation principle to the attention of the microbial research community.},
	language = {en},
	urldate = {2015-02-07},
	journal = {Scientifica},
	author = {Gloag, Erin S. and Turnbull, Lynne and Whitchurch, Cynthia B.},
	month = jan,
	year = {2015},
	pages = {e387342},
	file = {Gloag.EtAl.2015.BacterialStigmergyAnOrganizingPrinciple.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Gloag.EtAl.2015.BacterialStigmergyAnOrganizingPrinciple.pdf:application/pdf}
