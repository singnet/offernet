@incollection{fernando_design_2013-1,
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {Design for a {Darwinian} {Brain}: {Part} 1. {Philosophy} and {Neuroscience}},
	copyright = {©2013 Springer-Verlag Berlin Heidelberg},
