	file = {Heylighen.2013.SellfOrganizationInCommunicationgGroups.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Heylighen.2013.SellfOrganizationInCommunicationgGroups.pdf:application/pdf}
