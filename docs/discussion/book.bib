@incollection{fernando_design_2013,
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {Design for a {Darwinian} {Brain}: {Part} 2. {Cognitive} {Architecture}},
	copyright = {©2013 Springer-Verlag Berlin Heidelberg},
	isbn = {978-3-642-39801-8 978-3-642-39802-5},
	shorttitle = {Design for a {Darwinian} {Brain}},
	url = {http://link.springer.com/chapter/10.1007/978-3-642-39802-5_8},
	abstract = {The accumulation of adaptations in an open-ended manner during lifetime learning is a holy grail in reinforcement learning, intrinsic motivation, artificial curiosity, and developmental robotics. We present a design for a cognitive architecture that is capable of specifying an unlimited range of behaviors. We then give examples of how it can stochastically explore an interesting space of adjacent possible behaviors. There are two main novelties; the first is a proper definition of the fitness of self-generated games such that interesting games are expected to evolve. The second is a modular and evolvable behavior language that has systematicity, productivity, and compositionality, i.e. it is a physical symbol system. A part of the architecture has already been implemented on a humanoid robot.},
