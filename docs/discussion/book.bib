	file = {Ono.2000.SelfMaintainanceAndSelfReproductinInAbstractCellModel.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Ono.2000.SelfMaintainanceAndSelfReproductinInAbstractCellModel.pdf:application/pdf}
}

@book{murphy_big_2004,
