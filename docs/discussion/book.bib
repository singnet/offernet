@article{christos_investigation_1996,
	title = {Investigation of the {Crick}-{Mitchison} reverse-learning dream sleep hypothesis in a dynamical setting},
	volume = {9},
	issn = {0893-6080},
	url = {http://www.sciencedirect.com/science/article/pii/0893608095000720},
	doi = {10.1016/0893-6080(95)00072-0},
	abstract = {Crick and Mitchison have proposed that dream sleep involves a reverse-learning (or unlearning) mechanism by which the brain eliminates internal parasitic modes from memory and avoids the overloading catastrophe. We perform stimulations on the Hopfield neural network to test this hypothesis. In our dynamical model, the neural network is allowed to learn (while awake) and unlearn (while dreaming) in alternate cycles, in much the same was as in a biological system. Our results show that with the appropriate mix of learning and unlearning (dreaming) the neural network can continue to function for ever and ever, avoiding the overloading catastrophe. As found in other palimpsest models, the dynamic storage capacity in our model is lower in comparison to the static model. This would seem to suggest that the function of dream sleep may not be primarily concerned with improving the storage capacity of the brain as is commonly thought. On the basis of this we suggest that dream sleep may be more important as a means to gradually forget weak (that is, old, irrelevant and parasitic) memories.},
	number = {3},
	urldate = {2014-12-17},
	journal = {Neural Networks},
	author = {Christos, George A.},
	month = apr,
	year = {1996},
	keywords = {vveitas:generative\_model},
	pages = {427--434},
	file = {Chistos.1996.InvestigationOfTheCrickMitchisonReverseLearningDreamSleepHypothesisInADynamicalSetting.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Chistos.1996.InvestigationOfTheCrickMitchisonReverseLearningDreamSleepHypothesisInADynamicalSetting.pdf:application/pdf}
}

@book{goertzel_complexity_1997,
	title = {From {Complexity} to {Creativity}: {Explorations} in {Evolutionary}, {Autopoietic}, and {Cognitive} {Dynamics}},
	isbn = {978-0-306-45518-6},
	shorttitle = {From {Complexity} to {Creativity}},
	abstract = {Cybernetic pioneer Warren McCullough asked: "What is a man, that he may know a number; and what is a number, that a man may know it?" Thinking along much the same lines, my question here is: "What is a creative mind, that it might emerge from a complex system; and what is a complex system, that it might give rise to a creative mind?" Complexity science is a fashionable topic these days. My perspective on complexity, however, is a somewhat unusual one: I am interested in complex systems science principally as it reflects on abstract mathematical, computational models of mind. In my three previous books, The Structure of Intelligence, Evolving Mind, and Chaotic Logic, I have outlined a comprehensive complex-systems-theoretic theory of mind that I now call the psynet model. This book is a continuation of the research program presented in my previous books (and those books will be frequently referred to here, by the nicknames EM and CL). One might summarize the trajectory of thought spanning these four books as follows. SI formulated a philosophy and mathem- ics of mind, based on theoretical computer science and the concept of "pattern. " EM analyzed the theory of evolution by natural selection in similar terms, and used this computational theory of evolution to establish the evolutionary nature of thought.},
	language = {en},
	publisher = {Springer Science \& Business Media},
	author = {Goertzel, Ben},
	month = feb,
	year = {1997},
	file = {Goertzel.1997.FromComplexityToCreativityExplorationsInEvolutionaryAutopoieticAndCognitiveDynamics.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Goertzel.1997.FromComplexityToCreativityExplorationsInEvolutionaryAutopoieticAndCognitiveDynamics.pdf:application/pdf}
}

@article{lehman_investigating_2014,
	title = {Investigating {Biological} {Assumptions} through {Radical} {Reimplementation}},
	abstract = {An important goal in both artificial life and biology is uncovering the most general principles underlying life, which might catalyze both our understanding of life and engineering life-like machines. While many such general principles have been hypothesized, conclusively testing them is difficult because life on Earth provides only a singular example from which to infer. To circumvent this limitation, this paper formalizes an approach called radical reimplementation. The idea is to investigate an abstract biological hypothesis by intentionally reimplementing its main principles to diverge maximally from existing natural examples. If the reimplementation successfully exhibits properties resembling biology it may better support the underlying hypothesis than an alternative example inspired more directly by nature. The approach thereby provides a principled alternative to a common tradition of defending and minimizing deviations from nature in artificial life. This work reviews examples that can be interpreted through the lens of radical reimplementation to yield potential insights into biology despite having purposefully unnatural experimental setups. In this way, radical reimplementation can help renew the relevance of computational systems for investigating biological theory and can act as a practical philosophical tool to help separate the fundamental features of terrestrial biology from the epiphenomenal.},
	journal = {Artificial Life},
	author = {Lehman, Joel and Stanley, Kenneth O.},
	year = {2014},
	file = {Lehman.Stanley.2014.InvestigatingBiologicalAssumptionsThroughRadicalReimplemention.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Lehman.Stanley.2014.InvestigatingBiologicalAssumptionsThroughRadicalReimplemention.pdf:application/pdf}
}

@book{schimel_writing_2012,
	title = {Writing {Science}: {How} to {Write} {Papers} {That} {Get} {Cited} and {Proposals} {That} {Get} {Funded}},
	isbn = {978-0-19-976023-7},
	shorttitle = {Writing {Science}},
	abstract = {As a scientist, you are a professional writer: your career is built on successful proposals and papers. Success isn't defined by getting papers into print, but by getting them into the reader's consciousness. Writing Science is built upon the idea that successful science writing tells a story. It uses that insight to discuss how to write more effectively. Integrating lessons from other genres of writing with those from the author's years of experience as author, reviewer, and editor, the book shows scientists and students how to present their research in a way that is clear and that will maximize reader comprehension. The book takes an integrated approach, using the principles of story structure to discuss every aspect of successful science writing, from the overall structure of a paper or proposal to individual sections, paragraphs, sentences, and words. It begins by building core arguments, analyzing why some stories are engaging and memorable while others are quickly forgotten, and proceeds to the elements of story structure, showing how the structures scientists and researchers use in papers and proposals fit into classical models. The book targets the internal structure of a paper, explaining how to write clear and professional sections, paragraphs, and sentences in a way that is clear and compelling. The ideas within a paper should flow seamlessly, drawing readers along. The final section of the book deals with special challenges, such as how to discuss research limitations and how to write for the public. Writing Science is a much-needed guide to succeeding in modern science. Its insights and strategies will equip science students, scientists, and professionals across a wide range of scientific and technical fields with the tools needed to communicate effectively.},
	language = {en},
	publisher = {Oxford University Press},
	author = {Schimel, Joshua},
	year = {2012},
	file = {Schimel.2012WritingScienceHowtoWritePapers.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Schimel.2012WritingScienceHowtoWritePapers.pdf:application/pdf}
}

@unpublished{napp_message_2013,
	address = {Cambridge},
	title = {Message {Passing} {Inference} with {Chemical} {Reaction} {Networks}},
	abstract = {Recent work on molecular programming has explored new possibilities for computational
abstractions with biomolecules, including logic gates, neural networks,
and linear systems. In the future such abstractions might enable nanoscale devices
that can sense and control the world at a molecular scale. Just as in macroscale
robotics, it is critical that such devices can learn about their environment and reason
under uncertainty. At this small scale, systems are typically modeled as chemical
reaction networks. In this work, we develop a procedure that can take arbitrary
probabilistic graphical models, represented as factor graphs over discrete random
variables, and compile theminto chemical reaction networks that implement inference.
In particular, we show that marginalization based on sum-product message
passing can be implemented in terms of reactions between chemical species whose
concentrations represent probabilities. We show algebraically that the steady state
concentration of these species correspond to the marginal distributions of the random
variables in the graph and validate the results in simulations. As with standard
sum-product inference, this procedure yields exact results for tree-structured
graphs, and approximate solutions for loopy graphs},
	author = {Napp, Nils and Adams, Ryan Prescott},
	year = {2013},
	file = {Napp.Adams.2013.MessagePassingInferenceWithChemicalReactionNetworks.pdf:/media/data/vveitas/gdrive/vveitas@gmail.com/library/Napp.Adams.2013.MessagePassingInferenceWithChemicalReactionNetworks.pdf:application/pdf}
}

